  662  ls -lh
  663  rm -fr 1.txt 2.txt 3.txt
  664  ls -lh
  665  gzip samosa.tar
  666  ls -lh
  667  bzip2 samosa.tar.gz
  668  yum install bzip2
  669  bzip2 samosa.tar.gz
